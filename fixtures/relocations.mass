global_char := "a".bytes.0
global_char_pointer := &global_char

global_static_string :: "b"

test :: fn() {
  io :: import("std/io")
  char_string : String
  char_string.bytes = global_char_pointer
  char_string.length = 1
  io.print(char_string)
  io.print(global_static_string)
  io.print("\n")
}

main :: fn() {
  process :: import("std/process")
  test()
  process.exit(0)
}
