ExitProcess :: (status : s32) -> (s64) external("kernel32.dll", "ExitProcess")
GetStdHandle :: (handle : s32) -> (s64) external("kernel32.dll", "GetStdHandle")
WriteFile :: (
  handle : s64,
  buffer : [u8],
  size : s32,
  bytes_written : [s32],
  overlapped : [s64]
) -> (s64) external("kernel32.dll", "WriteFile")

main :: () -> () {
  STDOUT_HANDLE :: -11;
  message :: "Hello, World!"
  WriteFile(GetStdHandle(STDOUT_HANDLE), message.bytes, message.length, 0, 0);
  ExitProcess(0)
}
