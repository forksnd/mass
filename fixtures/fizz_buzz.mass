fizz_buzz :: fn() {
  io :: import("std/io")

  for i := 1; i <= 100; i = i + 1 {
    print_number := true
    if i % 3 == 0 then {
      print_number = false
      io.print("fizz")
    } else {}
    if i % 5 == 0 then {
      print_number = false
      io.print("buzz")
    } else {}
    if print_number then {
      io.print(i)
    } else {}
    io.print(" ")
  }
}

main :: fn() {
  process :: import("std/process")
  fizz_buzz()
  process.exit(0)
}
