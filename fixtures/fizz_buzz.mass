ExitProcess :: fn(status : s32) -> (s64) external("kernel32.dll", "ExitProcess")

fizz_buzz :: fn() {
  io :: import("std/io")

  for i : s8 = 1; i <= 100; i = i + 1 {
    print_number := true
    if (i % 3 == 0) {
      print_number = false
      io.print("fizz")
    }
    if (i % 5 == 0) {
      print_number = false
      io.print("buzz")
    }
    if (print_number) {
      io.print(i)
    }
    io.print(" ")
  }
}

main :: fn() {
  fizz_buzz()
  ExitProcess(0)
}
