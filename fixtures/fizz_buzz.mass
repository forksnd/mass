fizz_buzz :: fn() {
  io :: import("std/io")

  i := 1
  while i <= 100 {
    print_number := true
    if i % 3 == 0 then {
      print_number = false
      io.print("fizz")
    }
    if i % 5 == 0 then {
      print_number = false
      io.print("buzz")
    }
    if print_number then {
      io.print(i)
    }
    io.print(" ")
    i = i + 1
  }
}

main :: fn() {
  process :: import("std/process")
  fizz_buzz()
  process.exit(0)
}
